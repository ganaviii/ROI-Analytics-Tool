import { Bar } from 'react-chartjs-2';

const ROIChart = ({ roiData }) => {
    const data = {
        labels: ['Cost of Investment'],
        datasets: [{
            label: 'ROI Data',
            data: [roiData],
            backgroundColor: 'rgba(75, 192, 192, 0.6)',
        }],
    };
    return <Bar data={data} />;
};
